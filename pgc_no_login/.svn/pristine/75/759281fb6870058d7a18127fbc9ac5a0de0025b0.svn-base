<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/ic_ioffer_register_background" >

    <RelativeLayout
        android:id="@+id/top_bar"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/ioffer_top_bar_height"
        android:background="@drawable/ic_ioffer_title_bar_bg" >

        <TextView
            android:id="@+id/bnt_setting_rate_back"
            android:layout_width="60dp"
            android:layout_height="fill_parent"
            android:layout_centerVertical="true"
            android:layout_margin="2dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/ic_ioffer_top_bar_back_selector"
            android:clickable="true"
            android:gravity="center"
            android:onClick="buttonOnclick"
            android:text="@string/label_bnt_video_size"
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/level_content_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/label_setting_rate_title"
            android:textColor="@color/color_register_big_label"
            android:textSize="20sp"
            android:textStyle="bold" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/setting_rate_low"
        android:layout_width="fill_parent"
        android:layout_height="42dp"
        android:layout_below="@id/top_bar"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_ioffer_setting_item_start_gb"
        android:clickable="true"
        android:onClick="buttonOnclick" >

        <TextView
            android:id="@+id/video_rate_text_low"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="@string/label_setting_low"
            android:textColor="@color/color_setting_label"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/bnt_setting_rate_low"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_ioffer_level_default" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/line_1"
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:layout_below="@id/setting_rate_low"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/ic_ioffer_divide_line" />

    <RelativeLayout
        android:id="@+id/setting_rate_middle"
        android:layout_width="fill_parent"
        android:layout_height="42dp"
        android:layout_below="@id/line_1"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/ic_ioffer_setting_item_middle_gb"
        android:clickable="true"
        android:onClick="buttonOnclick" >

        <TextView
            android:id="@+id/video_rate_text_middle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="@string/label_setting_middle"
            android:textColor="@color/color_setting_label"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/bnt_setting_rate_middle"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_ioffer_level_default" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/line_2"
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:layout_below="@id/setting_rate_middle"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/ic_ioffer_divide_line" />

    <RelativeLayout
        android:id="@+id/setting_rate_high"
        android:layout_width="fill_parent"
        android:layout_height="42dp"
        android:layout_below="@id/line_2"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/ic_ioffer_setting_item_end_gb"
        android:clickable="true"
        android:onClick="buttonOnclick" >

        <TextView
            android:id="@+id/video_rate_text_high"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="@string/label_setting_high"
            android:textColor="@color/color_setting_label"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/bnt_setting_rate_high"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_ioffer_level_default" />
    </RelativeLayout>

</RelativeLayout>